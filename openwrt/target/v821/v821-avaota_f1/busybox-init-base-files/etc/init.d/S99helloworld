#!/bin/sh
#
# Start helloworld ....
#

start() {
      printf "Start helloworld .... "
      helloworld
}

stop() {
    printf "Stopping helloworld .... "
}

case "$1" in
    start)
    start
    ;;
    stop)
    stop
    ;;
    restart|reload)
    stop
    start
    ;;
  *)
    echo "Usage: $0 {start|stop|restart}"
    exit 1
esac

exit $?